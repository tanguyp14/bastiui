{% # theme-check-disable %}
{% comment %} Custom font: Dwight {% endcomment %}
<link
  rel="preload"
  as="font"
  href="{{ 'Dwight-Regular.otf' | asset_url }}"
  type="font/otf"
  crossorigin
>
<link
  rel="preload"
  as="font"
  href="{{ 'Dwight-Bold.otf' | asset_url }}"
  type="font/otf"
  crossorigin
>

{%- unless settings.type_body_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_body_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_subheading_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_subheading_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_heading_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_heading_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{%- unless settings.type_accent_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_accent_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{% # theme-check-enable %}
